{
  "name": "cs308project",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.1",
    "firebase": "^11.4.0",
    "html2pdf.js": "^0.10.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.5.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "vitest",
    "eject": "react-scripts eject",
    "test:watch": "react-scripts test --watch",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:ui": "vitest --ui",
    "test:mocha": "mocha --require @babel/register \"src/**/*.test.js\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "ignore-styles": "^5.0.1",
    "jest": "^27.5.1",
    "jsdom": "^26.1.0",
    "mocha": "^11.1.0",
    "mock-require": "^3.0.3",
    "sinon": "^20.0.0",
    "vitest": "^3.1.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "setupFiles": [
      "<rootDir>/test/setupfiles.js"
    ]
  }
}
